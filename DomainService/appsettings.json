{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "gRPC": {
      "Protocols": "Http2"
    }
  },
  "MongoDB": {
    "ConnectionString": "mongodb://mongo:27017",
    "DatabaseName": "CustomerDatabase"
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://guest:guest@localhost:5672"
  }
}
